! Set up xterm colors and characterics.
!
! Note to reload Xresources the following is required:
!   % xrdb -merge .Xresources

! Dim gray (usually defined as 'Black')
xterm-moonfly*color0: #373c40

! Red
xterm-moonfly*color1: #ff5454

! Green
xterm-moonfly*color2: #8cc85f

! Khaki (usually defined as 'Yellow')
xterm-moonfly*color3: #e3c78a

! Blue
xterm-moonfly*color4: #80a0ff

! Magenta
xterm-moonfly*color5: #ce76e8

! Turquoise (usually defined as 'Cyan')
xterm-moonfly*color6: #7ee0ce

! Orange (usally defined as 'White')
xterm-moonfly*color7: #de935f

! Coral (usually defined as 'Bold Black')
xterm-moonfly*color8: #f09479

! Crimson (usally defined as 'Bold Red')
xterm-moonfly*color9: #f74782

! Emerald (usually defined as 'Bold Green')
xterm-moonfly*color10: #42cf89

! Wheat (usually defined as 'Bold Yellow')
xterm-moonfly*color11: #cfcfb0

! Light blue (usually defined as 'Bold Blue')
xterm-moonfly*color12: #78c2ff

! Purple (usually defined as 'Bold Magenta')
xterm-moonfly*color13: #ae81ff

! Light green (usually defined as 'Bold Cyan')
xterm-moonfly*color14: #85dc85

! Violet (usually defined as 'Bold White')
xterm-moonfly*color15: #e2637f

xterm-moonfly*allowTitleOps: false
xterm-moonfly*background: #080808
xterm-moonfly*charClass: 33:48,35:48,37:48,42:48,45-47:48,58:48,61:48,63-64:48,95:48,126:48
xterm-moonfly*colorBD: #eeeeee
xterm-moonfly*colorBDMode: on
xterm-moonfly*colorUL: #42cf89
xterm-moonfly*colorULMode: on
xterm-moonfly*cursorColor: #9e9e9e
xterm-moonfly*cutNewline: false
xterm-moonfly*cutToBeginningOfLine: false
xterm-moonfly*faceName: Inconsolata
xterm-moonfly*faceSize: 12
xterm-moonfly*fastScroll: true
xterm-moonfly*foreground: #b2b2b2
xterm-moonfly*geometry: 84x65
xterm-moonfly*highlightSelection: true
xterm-moonfly*loginShell: true
xterm-moonfly*metaSendsEscape: true
xterm-moonfly*saveLines: 3000
xterm-moonfly*scrollBar: false
xterm-moonfly*scrollbar*background: #292929
xterm-moonfly*scrollbar*foreground: #b2b2b2
xterm-moonfly*scrollbar*width: 10
xterm-moonfly*termName: xterm-256color

! Get nice keys such as home/end working in xterms.
! Shift-Control C/V for copy/paste to CLIPBOARD.
! Shift Up/Down arrow for scrolling one line at a time.
*VT100.Translations: #override \
    <Key>Home: string("\033[1~")\n\
    <Key>End: string("\033[4~")\n\
    Shift Ctrl <Key>C: copy-selection(CLIPBOARD)\n\
    Shift Ctrl <Key>V: insert-selection(CLIPBOARD)\n\
    Shift <Key>Up: scroll-back(1)\n\
    Shift <Key>Down: scroll-forw(1)

! Font related settings.
Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
